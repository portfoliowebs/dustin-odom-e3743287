{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "builds": [
    {
      "src": "src/index.tsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [],
  "headers": [],
  "rewrites": []
}